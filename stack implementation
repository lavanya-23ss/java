void push (int val,int n) //n is size of the stack   
{  
    if (top == n )   
    printf("\n Overflow");   
    else   
    {  
    top = top +1;   
    stack[top] = val;   
    }   
}   


Deletion 
begin   
    if top = 0 then stack empty;   
    item := stack(top);  
    top = top - 1;  
end;    
